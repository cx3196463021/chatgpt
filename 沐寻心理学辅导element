            if ("https%3A%2F%2Fmooc1-2.chaoxing.com%2Fmycourse%2Fstudentstudy%3FchapterId%3D626380896%26courseId%3D227419249%26clazzid%3D73306336%26enc%3Dcffaa91f745127dd5dd57c04d03918a2".indexOf("dayi100.com") != -1) {
