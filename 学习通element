    function to_third_login(type,ref){&#13;
        var fd=$("#fid").val();&#13;
        if ("qq" == type) {&#13;
            window.location.href="/oauth2/qq?newversion=true&amp;fid="+fd+"&amp;refer="+$("#refer").val();&#13;
        } else if("weixin" == type){&#13;
            window.location.href="/oauth2/weixin?newversion=true&amp;fid="+fd+"&amp;refer="+$("#refer").val();&#13;
        }&#13;
    }&#13;
