    let capInstance  = null;//密码滑动校验用
    // let twoLogincapInstance  = null;//双因子登录滑动校验用
    $(function () {
        initTelList();
        let needVcode = $("#needVcode").val();
        if (needVcode == "1") {
            loadSlide(null, function(){
                loginByPhoneAndPwdSubmit();
            });
        }
    })
</script>
