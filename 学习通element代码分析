    if (userAgent.indexOf("SSREADER/")>-1){
        let versionArr = userAgent.substr(0,10).split("/");
        if (versionArr[1] >= 4){
            hideQRcode();
        }
    }

    //定制不显示二维码
    if( 1 != 1 || 1 != 1){
        hideQRcode();
    }else if(994 == 34415){
        $("#quickCode").attr("src","images/guide/34415ewm.png");
