            if (loginTip != "undefined" && loginTip.trim() != "") {
                $("#loginTip").show();
            }

            if ("https%3A%2F%2Fmooc1-2.chaoxing.com%2Fmycourse%2Fstudentstudy%3FchapterId%3D711689721%26courseId%3D232704535%26clazzid%3D72235500%26enc%3D99389dd934222fa89f3ed2b878fca907".indexOf("duxiu.com") != -1 || "https%3A%2F%2Fmooc1-2.chaoxing.com%2Fmycourse%2Fstudentstudy%3FchapterId%3D711689721%26courseId%3D232704535%26clazzid%3D72235500%26enc%3D99389dd934222fa89f3ed2b878fca907".indexOf("blyun.com") != -1 || "https%3A%2F%2Fmooc1-2.chaoxing.com%2Fmycourse%2Fstudentstudy%3FchapterId%3D711689721%26courseId%3D232704535%26clazzid%3D72235500%26enc%3D99389dd934222fa89f3ed2b878fca907".indexOf("zhizhen.com") != -1) {
